grid:
  type: RectangleMesh
  nx: 2
  ny: 2
  xmin: 0
  xmax: 2
  ymin: 0
  ymax: 2

auxs:
  forcing_fn:
    type: FunctionAuxiliaryField
    value: '-4'

problem:
  type: PoissonFENonlinearProblem

ics:
  all:
    type: ConstantIC
    value: [ 0 ]

bcs:
  all:
    type: DirichletBC
    boundary: 'marker'
    value: [ 'x*x + y*y' ]

output:
  hdf5:
    type: HDF5Output
    file: fe_2d
