mesh:
  type: BoxMesh
  nx: 2
  ny: 2
  nz: 2
  xmin: 0
  xmax: 2
  ymin: 0
  ymax: 2
  zmin: 0
  zmax: 2

auxs:
  forcing_fn:
    type: FunctionAuxiliaryField
    value: '-6'

problem:
  type: PoissonEquation

ics:
  all:
    type: ConstantInitialCondition
    field: 'u'
    value: 0

bcs:
  left:
    type: DirichletBC
    boundary: 'left'
    value: 'x*x + y*y + z*z'

  right:
    type: DirichletBC
    boundary: 'right'
    value: 'x*x + y*y + z*z'

  top:
    type: DirichletBC
    boundary: 'top'
    value: 'x*x + y*y + z*z'

  bottom:
    type: DirichletBC
    boundary: 'bottom'
    value: 'x*x + y*y + z*z'

  front:
    type: DirichletBC
    boundary: 'front'
    value: 'x*x + y*y + z*z'

  back:
    type: DirichletBC
    boundary: 'back'
    value: 'x*x + y*y + z*z'

output:
  out:
    type: ExodusIIOutput
    variables: 'u'
