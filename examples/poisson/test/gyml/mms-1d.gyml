mesh:
  type: LineMesh
  nx: 2
  xmin: 0
  xmax: 2

auxs:
  forcing_fn:
    type: FunctionAuxiliaryField
    value: '-2'

problem:
  type: PoissonEquation

ics:
  all:
    type: ConstantIC
    value: 0

bcs:
  left:
    type: DirichletBC
    boundary: 'left'
    value: [ 'x*x' ]

  right:
    type: DirichletBC
    boundary: 'right'
    value: [ 'x*x' ]

output:
  out:
    type: ExodusIIOutput
    variables: 'u'
